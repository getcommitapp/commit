{
  "name": "@commit/mobile",
  "main": "expo-router/entry",
  "version": "0.4.1",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "format": "prettier --write . --ignore-path ../../.prettierignore",
    "lint": "expo lint",
    "release:bump": "node -e \"const f=require('fs'),v=process.env.VERSION;if(!v){console.error('Usage: VERSION=x.y.z pnpm release:bump');process.exit(1);}['package.json','app.json'].forEach(p=>{const j=JSON.parse(f.readFileSync(p,'utf8'));p==='app.json'?j.expo.version=v:j.version=v;f.writeFileSync(p,JSON.stringify(j,null,2)+'\\n')});console.log('Bumped to',v);\""
  },
  "jest": {
    "preset": "jest-expo",
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/__mocks__/svgMock.js",
      "^react$": "<rootDir>/node_modules/react",
      "^react-native$": "<rootDir>/node_modules/react-native"
    },
    "transformIgnorePatterns": [
      "node_modules/.pnpm/(?!(@react-native|react-native|expo-modules-core|expo|@expo|expo-router|expo-linking|expo-font|expo-asset|react-native-web|nanostores|better-auth|@better-auth|react-native-safe-area-context))"
    ]
  },
  "dependencies": {
    "@better-auth/expo": "^1.3.7",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.1.6",
    "better-auth": "^1.3.7",
    "expo": "~53.0.22",
    "expo-apple-authentication": "^7.2.4",
    "expo-auth-session": "^6.2.1",
    "expo-constants": "^17.1.7",
    "expo-font": "~13.3.2",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.5",
    "expo-secure-store": "^14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "13.3.1",
    "@types/jest": "29.5.14",
    "@types/react": "~19.0.10",
    "eslint": "^9.33.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.10",
    "prettier": "^3.6.2",
    "react-native-svg-transformer": "^1.5.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
